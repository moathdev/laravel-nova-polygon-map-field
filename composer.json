{
    "name": "moathdev/laravel-nova-polygon-map-field",
    "description": "Laravel Nova Polygon map field.",
    "keywords": [
        "laravel",
        "nova",
        "polygon",
        "map"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "grimzy/laravel-mysql-spatial": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Moathdev\\PolygonMap\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moathdev\\PolygonMap\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
